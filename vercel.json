{
  "version": 2,
  "builds": [
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/sw.js", "dest": "/public/sw.js", "headers": { "Service-Worker-Allowed": "/", "Cache-Control": "no-cache" } },
    { "src": "/manifest.json", "dest": "/public/manifest.json" },
    { "src": "/icon-192.png", "dest": "/public/icon-192.png" },
    { "src": "/icon-512.png", "dest": "/public/icon-512.png" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}
